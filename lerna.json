{
  "command": {
    "version": {
      "allowBranch": "main",
      "message": "(release) @otjs/*: Publish %s for all Packages"
    }
  },
  "ignoreChanges": [
    "**/__mocks__/**",
    "**/__tests__/**",
    "**/*.md",
    "**/*.yml"
  ],
  "npmClient": "yarn",
  "npmClientArgs": ["--frozen-lockfile"],
  "packages": ["packages/*"],
  "useWorkspaces": true,
  "version": "0.1.1"
}
